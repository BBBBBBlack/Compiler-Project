{
    "parseTabFile": "test/in/test3_2tab.md",
    "copy": [
        "#include <iostream>",
        "#include <string>",
        "using namespace std;"
    ],
    "rules": [
        {
            "left": "E",
            "rights": [
                {
                    "right": "E '*' E",
                    "action": [
                        "$$ = to_string(stoi($0) * stoi($2));"
                    ]
                },
                {
                    "right": "E '+' E",
                    "action": [
                        "$$ = to_string(stoi($0) + stoi($2));"
                    ]
                },
                {
                    "right": "'digit'",
                    "action": [
                        "$$ = $0;"
                    ]
                }
            ]
        }
    ]
}